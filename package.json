{
  "name": "libxslt-wasm",
  "version": "0.0.10",
  "type": "module",
  "description": "JavaScript bindings for libxslt compiled to WebAssembly",
  "scripts": {
    "prepare": "husky",
    "typecheck": "tsc --noEmit",
    "build": "tsc",
    "compile": "./scripts/compile.sh",
    "test": "NODE_OPTIONS='--import ./setup.js --experimental-vm-modules' jest",
    "test:watch": "node --run test -- --watch",
    "docker:build": "./scripts/build.sh",
    "docker:run": "./scripts/run.sh",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check"
  },
  "keywords": [
    "libxslt",
    "libxml2",
    "wasm",
    "webassembly",
    "xml",
    "xslt",
    "exslt",
    "xsl"
  ],
  "homepage": "https://github.com/jeremy-code/libxslt-wasm#readme",
  "bugs": "https://github.com/jeremy-code/libxslt-wasm/issues",
  "license": "MIT",
  "author": "Jeremy Nguyen <nguyen.jeremyt@gmail.com> (https://jeremy.ng)",
  "repository": "github:jeremy-code/libxslt-wasm",
  "engines": {
    "node": ">=19.2.0"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    },
    "./*.js": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    },
    "./exslt": {
      "types": "./dist/exslt/index.d.ts",
      "default": "./dist/exslt/index.js"
    },
    "./exslt/*": {
      "types": "./dist/exslt/*.d.ts",
      "default": "./dist/exslt/*.js"
    },
    "./exslt/*.js": {
      "types": "./dist/exslt/*.d.ts",
      "default": "./dist/exslt/*.js"
    },
    "./internal": {
      "types": "./dist/internal/index.d.ts",
      "default": "./dist/internal/index.js"
    },
    "./internal/*": {
      "types": "./dist/internal/*.d.ts",
      "default": "./dist/internal/*.js"
    },
    "./internal/*.js": {
      "types": "./dist/internal/*.d.ts",
      "default": "./dist/internal/*.js"
    },
    "./parser/*": {
      "types": "./dist/parser/*.d.ts",
      "default": "./dist/parser/*.js"
    },
    "./parser/*.js": {
      "types": "./dist/parser/*.d.ts",
      "default": "./dist/parser/*.js"
    },
    "./output/libxslt": {
      "types": "./dist/output/libxslt.d.ts",
      "default": "./dist/output/libxslt.js"
    },
    "./output/libxslt.js": {
      "types": "./dist/output/libxslt.d.ts",
      "default": "./dist/output/libxslt.js"
    },
    "./output/libxslt.wasm": "./dist/output/libxslt.wasm"
  },
  "imports": {
    "#libxslt": {
      "types": "./dist/output/libxslt.d.ts",
      "default": "./dist/output/libxslt.js"
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@jest/globals": "^29.7.0",
    "@types/node": "^22.15.2",
    "eslint": "^9.25.1",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-import-x": "^4.11.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-n": "^17.17.0",
    "eslint-plugin-promise": "^7.2.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-config": "^29.7.0",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0"
  },
  "packageManager": "pnpm@10.9.0+sha512.0486e394640d3c1fb3c9d43d49cf92879ff74f8516959c235308f5a8f62e2e19528a65cdc2a3058f587cde71eba3d5b56327c8c33a97e4c4051ca48a10ca2d5f"
}
